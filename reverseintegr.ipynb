   "source": [
    "def reverse(num):\n",
    "    sign=-1 if num<0 else 1\n",
    "    num=abs(num)\n",
    "\n",
    "    reverse =0\n",
    "    while num >0:\n",
    "        digit=num%10\n",
    "        reverse=reverse*10 + digit\n",
    "        num=num //10\n",
    "\n",
    "    return sign*reverse"
   ]
