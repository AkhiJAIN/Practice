   "id": "cb367933",
   "metadata": {},
   "outputs": [],
   "source": [
    "def string(s):\n",
    "    reverse=\"\"\n",
    "\n",
    "    for ch in s:\n",
    "        reverse=ch+reverse\n",
    "    return reverse\n"
   ]
  },
